    "axios": "^1.8.3",
    "canvas": "^3.1.0",
    "framer-motion": "^10.16.4",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.474.0",
    "marked": "^15.0.7",
    "pdfjs-dist": "^5.3.31",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
